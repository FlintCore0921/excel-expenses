<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" pickOnBounds="false" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.flintcore.excel_expenses.controllers.SidebarController">
    <content>
        <VBox id="navbar" cache="true" cacheHint="SPEED">
            <children>
                <HBox styleClass="navbar-title-holder">
                    <children>
                        <VBox HBox.hgrow="ALWAYS">
                            <children>
                                <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="37.0" prefWidth="93.0" text="Menu">
                                    <font>
                                        <Font name="Segoe UI Bold Italic" size="30.0" />
                                    </font>
                                </Label>
                            </children>
                            <HBox.margin>
                                <Insets bottom="10.0" left="5.0" right="5.0" top="5.0" />
                            </HBox.margin>
                        </VBox>
                    </children>
                </HBox>
                <VBox fx:id="navbarActions" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
            </children>
        </VBox>
    </content>
    <styleClass>
        <String fx:value="white-text" />
        <String fx:value="no-insets" />
    </styleClass>
</ScrollPane>
